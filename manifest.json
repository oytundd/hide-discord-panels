
{
  "name": "Hide Discord Panels",
  "version": "1",
  "manifest_version": 2,
  "description": "You can hide channel list and server list using this extension.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Hide Panels"
  },
  "icons": { "16": "icon.png",
    "48": "icon.png",
   "128": "icon.png" },
  "permissions": [
    "https://discord.com/channels/*"
  ]
}